<view class="app-page">
  <view class='table'>
    <view class="tr bg-w">
      <view class="th">车牌号</view>
      <view class="th">状态</view>
      <view class="th">申请编号</view>
      <view class="th">盘点时间</view>
    </view>
  </view>
  
  <block wx:for="{{listData}}" wx:key="item">
    <view class="tr" style="font-size:15px;">
      <view class="td">{{item.CAR_NO}}</view>
      <view class="td">{{item.STATUS}}</view>
      <view class="td">{{item.APPLY_NO}}</view>
      <view class="td">{{item.UPDATE_TIME}}</view>
    </view>
  </block>
</view>