<view class="app-page">
  <view class="page-content">
    <form>
      <view class="list-block">
        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">客户姓名</view>
            <view class="item-input">
              <input name="userName" disabled="true" type="text" value="{{obj.applyName}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">申请编号</view>
            <view class="item-input">
              <input name="userName" disabled="true" type="text" value="{{obj.applyNo}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车架号</view>
            <view class="item-input">
              <input name="mobile" disabled="true" placeholderClass="input-placeholder" type="number"
                value="{{obj.vin}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车牌号</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number"
                value="{{obj.carNumber}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车型</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number"
                value="{{obj.carModel}}"></input>
            </view>
          </view>
        </view>


        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车颜色</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number"
                value="{{obj.carColour}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">停放位置</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number"
                value="{{obj.stopSite}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label" wx:if="{{obj.putIn=='已入库'}}">入库日期</view>
            <view class="item-title label" wx:else="{{obj.putIn==''}}">出库日期</view>

            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number"
                value="{{obj.putInDate}}"></input>
            </view>
          </view>
        </view>

      </view>
    </form>
  </view>
</view>