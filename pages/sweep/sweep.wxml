<view class="app-page">
  <view class="page-content">
    <view class="content-block-title" style="margin-top:10px;">请认真阅读，并选择您的操作</view>
    <form bindsubmit="operaCarStock">
      <view class="list-block">

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">申请编号</view>
            <view class="item-input">
              <input name="userName" disabled="true" type="text" value="{{basqbh}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车架号</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{bacjhm}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车牌号</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{carNo}}"></input>
            </view>
          </view>
        </view>
        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">制造商</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{baclzz}}"></input>
            </view>
          </view>
        </view>


        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车系</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{userPhone}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车型</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{bacx}}"></input>
            </view>
          </view>
        </view>


        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">车颜色</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{carcolor}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">停放位置</view>
            <view class="item-input">
              <input maxlength="11" name="mobile" disabled="true" placeholderClass="input-placeholder" type="number" value="{{address}}"></input>
            </view>
          </view>
        </view>

        <view class="item-content">
          <view class="item-inner">
            <view class="item-title label">操作</view>
            <view class="item-input">
              <checkbox-group name="checkbox" bindchange="checkboxChange">
                <label wx:for ="{{actions}}">
                  <checkbox value="{{item.realValue}}" />{{item.name}}</label>
                  
              </checkbox-group>
            </view>
          </view>
        </view>

        <view class="content-block" style="margin:35px 0 5px">
          <button class="button button-big jmed-active" formType="submit" style="width: 70%;margin:0 auto">确定</button>
        </view>
      </view>
    </form>
  </view>
</view>